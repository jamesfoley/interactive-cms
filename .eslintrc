{
  "extends": "standard",
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "modules": true,
  },
  "rules": {
    "arrow-parens": [2, "always"],
    "no-new": [0],
    "no-var": [2],
    "object-shorthand": [2, "always"],
    "prefer-const": [1],
    "prefer-template": [2],
    "no-console": [1]
  }
}
